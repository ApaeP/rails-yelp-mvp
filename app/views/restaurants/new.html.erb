<h2 class="text-center">New restaurant</h2>
<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :category %>
  <%= f.input :phone_number %>
  <%= f.submit "Create", class: "btn btn-info" %>
<% end %>

<%= link_to 'Back to Index', restaurants_path, class: "btn btn-info" %>
